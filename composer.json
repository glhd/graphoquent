{
  "name": "galahad/graphoquent",
  "description": "Expose your eloquent models as GraphQL queries and mutations",
  "keywords": [
    "laravel",
    "graphql",
    "eloquent"
  ],
  "authors": [
    {
      "name": "Chris Morrell",
      "homepage": "http://cmorrell.com"
    }
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.6",
    "illuminate/support": "5.3.*|5.4.*|5.5.*",
    "illuminate/database": "5.3.*|5.4.*|5.5.*",
    "illuminate/routing": "5.3.*|5.4.*|5.5.*",
    "illuminate/http": "5.3.*|5.4.*|5.5.*",
    "webonyx/graphql-php": "~0.9.13",
    "phpdocumentor/reflection-docblock": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7.19"
  },
  "autoload": {
    "psr-4": {
      "Galahad\\Graphoquent\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/stubs/"
    ],
    "psr-4": {
      "Galahad\\Graphoquent\\Tests\\": "tests/"
    }
  }
}
